{
  "name": "music-metadata",
  "description": "Streaming music metadata parser for node and the browser.",
  "version": "0.11.3",
  "author": {
    "name": "Borewit",
    "url": "https://github.com/Borewit"
  },
  "contributors": [
    {
      "name": "Lee Treveil",
      "email": "leetreveil@gmail.com",
      "url": "https://github.com/leetreveil"
    }
  ],
  "keywords": [
    "music",
    "metadata",
    "meta",
    "audio",
    "tag",
    "tags",
    "MusicBrainz",
    "Discogs",
    "Picard",
    "ID3",
    "ID3v1",
    "ID3v2",
    "m4a",
    "mp3",
    "mp4",
    "Vorbis",
    "ogg",
    "flac",
    "asf",
    "wma",
    "wmv",
    "tag",
    "ape",
    "MonkeyAudio",
    "aiff",
    "wav",
    "WavPack",
    "Opus"
  ],
  "main": "lib",
  "typings": "lib/index",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "del-cli lib/** src/**/*.js src/**/*.js.map src/**/*.d.ts test/**/*.js test/**/*.js.map",
    "compile-src": "tsc -p src",
    "compile-test": "tsc -p test",
    "compile-doc": "tsc -p doc-gen",
    "compile": "npm run compile-src && npm run compile-test && npm run compile-doc",
    "lint": "tslint 'src/**/*.ts' --exclude 'src/**/*.d.ts' 'test/**/*.ts' --exclude 'test/**/*.d.ts' 'doc-gen/**/*.ts'",
    "test": "mocha --require ts-node/register test",
    "prepare": "npm run clean && npm run compile && npm run doc-gen",
    "start": "npm-run-all compile lint cover-test",
    "cover-test": "nyc npm run test",
    "coveralls": "npm run cover-test && nyc report --reporter=text-lcov | coveralls",
    "doc-gen": "node doc-gen/gen.js"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "debug": "^3.1.0",
    "fs-extra": "^5.0.0",
    "strtok3": "^1.4.1",
    "then-read-stream": "^1.1.1",
    "token-types": "^0.9.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/fs-extra": "^5.0.2",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.8",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "del-cli": "^1.1.0",
    "hoek": "~> 4.2.1",
    "mime": "^2.3.1",
    "mocha": "^5.1.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.7.1",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "testling": {
    "files": [
      "test/test-js-*.js",
      "test/js-*.js"
    ],
    "browsers": [
      "firefox/latest",
      "chrome/latest",
      "safari/latest"
    ]
  },
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/borewit/music-metadata.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Borewit/music-metadata/issues"
  },
  "nyc": {
    "check-coverage": true,
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "coverage"
  }
}
