{
  "name": "streaming-http-token-reader",
  "version": "0.1.0",
  "description": "HTTP-tokenizer-client",
  "main": "lib/streaming-http-token-reader",
  "typings": "lib/streaming-http-token-reader",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Borewit/streaming-http-token-reader.git"
  },
  "author": {
    "name": "Borewit",
    "url": "https://github.com/Borewit"
  },
  "keywords": [
    "xhr",
    "http",
    "sttok3"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Borewit/streaming-http-token-reader/issues"
  },
  "scripts": {
    "clean": "del-cli lib/**/*.js lib/**/*.js.map lib/**/*.d.ts test/**/*.js test/**/*.js.map coverage",
    "compile-lib": "tsc -p lib/tsconfig.json",
    "compile-test": "tsc -p lib/tsconfig.spec.json",
    "compile": "npm run compile-lib && yarn run compile-test",
    "prepublishOnly": "yarn run build",
    "build": "npm run clean && yarn run compile",
    "lint": "tslint 'lib/**/*.ts' --exclude 'lib/**/*.d.ts' 'test/**/*.ts' --exclude 'test/**/*.d.ts'",
    "karma": "karma start",
    "test": "npm run karma-once",
    "karma-once": "karma start --browsers Chrome --single-run",
    "travis-karma": "karma start --browsers Firefox --single-run",
    "post-coveralls": "coveralls < coverage/lcov.info"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.8",
    "@types/node": "^12.0.7",
    "coveralls": "^3.0.2",
    "del-cli": "^3.0.0",
    "filereader-stream": "^2.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.3.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "music-metadata-browser": "^1.1.1",
    "ts-loader": "^6.0.2",
    "tslint": "^5.11.0",
    "typescript": "^3.3.1",
    "webpack": "^4.20.2"
  },
  "dependencies": {
    "debug": "^4.1.0",
    "strtok3": "^3.0.0"
  }
}
